{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . && stylelint \"**/*.{css,vue}\"",
    "lint:fix": "eslint . --fix && stylelint \"**/*.{css,vue}\" --fix",
    "lint:style": "stylelint \"**/*.{css,vue}\"",
    "lint:style:fix": "stylelint \"**/*.{css,vue}\" --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.14.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.8",
    "@nuxt/ui": "^3.2.0",
    "@unhead/vue": "^2.0.11",
    "nuxt": "^3.17.5",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/test-utils": "^3.19.1",
    "@stylistic/stylelint-plugin": "^3.1.3",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "stylelint": "^16.21.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{css,vue}": [
      "stylelint --fix"
    ]
  }
}
